#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mattia/code/grades/client/node_modules/touch/bin/node_modules:/home/mattia/code/grades/client/node_modules/touch/node_modules:/home/mattia/code/grades/client/node_modules:/home/mattia/code/grades/node_modules:/home/mattia/code/node_modules:/home/mattia/node_modules:/home/node_modules:/node_modules:/home/mattia/code/grades/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mattia/code/grades/client/node_modules/touch/bin/node_modules:/home/mattia/code/grades/client/node_modules/touch/node_modules:/home/mattia/code/grades/client/node_modules:/home/mattia/code/grades/node_modules:/home/mattia/code/node_modules:/home/mattia/node_modules:/home/node_modules:/node_modules:/home/mattia/code/grades/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
